<div class="col-sm-6 col-sm-offset-3">
<div class="panel panel-default">
<div class="panel-heading">



    <h1><span class="fa fa-sign-in"></span>Create Profile Reports</h1>

    <!-- show any messages that come back with authentication -->
    <% if (message.length > 0) { %>
        <div class="alert alert-danger"><%= message %></div>
    <% } %></div>
	<div class="panel-body">
    <!-- LOGIN FORM -->
    <form >

            
        <div class="form-group">
            <label>Email</label>
            <input ng-model="email" type="email" class="form-control" name="email">
        </div>
        <div class="form-group">
            <label>Add Report_1 URL</label>
            <input ng-model="myreports1" type="text" class="form-control" name="myreports1">
        </div>

         <div class="form-group">
            <label>Add Report_2 URL</label>
            <input ng-model="myreports2" type="text" class="form-control" name="myreports2">
        </div>
		 <div class="form-group">
            <label>Add Report_3 URL</label>
            <input ng-model= "myreports3"type="text" class="form-control" name="myreports3">
        </div>
		 <div class="form-group">
            <label>Add Report_4 URL</label>
            <input ng-model="myreports4" type="text" class="form-control" name="myreports14">
        </div>
		 <div class="form-group">
            <label>Add Report_5 URL</label>
            <input ng-model="myreports5" type="text" class="form-control" name="myreports5">
        </div>
   
        <button ng-click="proReports()"type="submit" class="btn btn-warning btn-lg">Submit</button>
    </form>
<h3><span ng-show="added">Profile Reports Added</span></h3>
		<h3><span ng-show="Notadded">Profile Reports Not Added</span></h3>
    <hr>

   
</div>
	</div>
</div>