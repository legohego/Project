<div class="col-sm-6 col-sm-offset-3">
<div class="panel panel-default">
<div class="panel-heading">



    <h1><span class="fa fa-sign-in"></span>Add New Report</h1>

    <!-- show any messages that come back with authentication -->
    <% if (message.length > 0) { %>
        <div class="alert alert-danger"><%= message %></div>
    <% } %></div>
	<div class="panel-body">
    <!-- LOGIN FORM -->
    <form action="/addReports" method="post">

            
        <div class="form-group">
            <label>Email</label>
            <input type="text" class="form-control" name="email">
        </div>
        <div class="form-group">
            <label>Add Report_1 URL</label>
            <input type="password" class="form-control" name="myreports1">
        </div>

         <div class="form-group">
            <label>Add Report_2 URL</label>
            <input type="password" class="form-control" name="myreports2">
        </div>
		 <div class="form-group">
            <label>Add Report_3 URL</label>
            <input type="password" class="form-control" name="myreports3">
        </div>
		 <div class="form-group">
            <label>Add Report_4 URL</label>
            <input type="password" class="form-control" name="myreports14">
        </div>
		 <div class="form-group">
            <label>Add Report_5 URL</label>
            <input type="password" class="form-control" name="myreports5">
        </div>
   
        <button type="submit" class="btn btn-warning btn-lg">Register</button>
    </form>

    <hr>

   
</div>
	</div>
</div>