<div class="col-sm-6 col-sm-offset-3">
<div class="panel panel-default">
<div class="panel-heading">



    <h1><span class="fa fa-sign-in"></span>Add Personnel Report</h1>

    <!-- show any messages that come back with authentication -->
    <% if (message.length > 0) { %>
        <div class="alert alert-danger"><%= message %></div>
    <% } %></div>
	<div class="panel-body">
    <!-- LOGIN FORM -->
    <form >

            
        <div class="form-group">
            <label>Email</label>
            <input ng-model="email" type="text" class="form-control" name="email">
        </div>
        <div class="form-group">
            <label>Add Report URL</label>
            <input ng-model="reportURL"type="text" class="form-control" name="reportURL">
        </div>

        
        <div class="form-group">
  <label for="sel1">Reports Number:</label>
  <select ng-model="reportNumber"class="form-control" id="sel1" name="reportNumber">
    <option>myreports1</option>
    <option>myreports2</option>
    <option>myreports3</option>
    <option>myreports4</option>
	<option>myreports5</option>
    <option>myreports6</option>
    <option>myreports7</option>
    <option>myreports8</option>
  </select>
</div>
        <button ng-click="proReportsadd()"type="submit" class="btn btn-warning btn-lg">Submit</button>
    </form>
<h3><span ng-show="added2">Profile Reports Added</span></h3>
		<h3><span ng-show="Notadded">Profile Reports Not Added</span></h3>

    <hr>

   
</div>
	</div>
</div>